{
  "name": "zfegg/api-serializer-ext",
  "description": "Serialize hal/json-ld/json-api with Symfony Serializer.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Moln Xie",
      "email": "moln.xie@gmail.com"
    }
  ],

  "autoload": {
    "psr-4": {
      "Zfegg\\ApiSerializerExt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZfeggTest\\ApiSerializerExt\\": "test/"
    }
  },
  "require": {
    "php": ">=7.4",
    "symfony/serializer": "^5.2|^6.0"
  },
  "extra": {
    "laminas": {
      "config-provider": "Zfegg\\ApiSerializerExt\\ConfigProvider"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
